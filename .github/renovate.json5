{
  "extends": [
    "config:base"
  ],
  "automerge": true,
  "automergeType": "branch",
  "automergeStrategy": "auto",
  "branchPrefix": "renovate/",
  "labels": ["dependencies"],
  "prCreation": "immediate",
  "dependencyDashboard": false,
  "packageRules": [
    {
      "automerge": true,
      "matchUpdateTypes": [
        "major",
        "minor",
        "patch",
        "pin",
        "digest",
        "lockFileMaintenance"
      ]
    }
  ],
  "rebaseWhen": "conflicted",
  "timezone": "UTC",
  "platformAutomerge": true
}